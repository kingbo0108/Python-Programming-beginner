x = int(input("please input a num:"))
y = int(input("please input a num:"))
r = float(input("please input a num radius  "))
print("x =",x)
print("y =",y)
print("r = ",r)
print("x + y =",x+y)
print("面積 = ",3.14*r*r)
print("周長 = ",2* 3.14*r)
